{
  "name": "duco",
  "version": "0.0.1",
  "description": "Service Wizard for web and customer service.",
  "main": "index.js",
  "engines": {
    "npm": ">=8.3.1",
    "node": ">=16.14.0"
  },
  "scripts": {
    "prestart": "npm run migrate",
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node duco-server/src/index.ts",
    "watch": "npm-run-all -p watch:*",
    "watch:client": "npm start --prefix duco-client",
    "watch:server": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development nodemon duco-server/src/index.ts",
    "build": "npm run build --prefix duco-client",
    "migrate": "knex migrate:latest",
    "lint": "npm-run-all lint:*",
    "lint:client": "npm run lint --prefix duco-client",
    "lint:server": "eslint duco-server",
    "format": "prettier .",
    "test": "jest",
    "db-reset": "docker-compose down -v && docker-compose up -d && sleep 7 && npx knex migrate:latest && npx knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.is.comhem.com:7999/tcs/duco.git"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/jest": "27.0.1",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "jest": "27.1.1",
    "nock": "13.1.3",
    "nodemon": "2.0.15",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "supertest": "6.1.6",
    "testcontainers": "7.21.0",
    "ts-jest": "27.1.4"
  },
  "author": "Tele2",
  "license": "ISC",
  "dependencies": {
    "@tele2se/log4js-logstash-tcp": "1.0.1",
    "@types/micromatch": "4.0.2",
    "axios": "0.21.4",
    "connect-history-api-fallback": "1.6.0",
    "cookie-session": "1.4.0",
    "express": "4.17.1",
    "express-async-handler": "1.2.0",
    "express-openapi-validator": "4.13.0",
    "knex": "0.95.6",
    "ldapjs": "2.3.1",
    "log4js": "6.3.0",
    "micromatch": "4.0.4",
    "moment": "2.29.3",
    "mysql": "2.18.1",
    "openapi-types": "9.3.0",
    "pg": "8.7.1",
    "short-uuid": "4.2.0",
    "swagger-ui-express": "4.1.6",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "uuid": "8.3.2"
  }
}
